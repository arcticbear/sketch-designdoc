@import "library.cocoascript";

var onRun = function(context) {
  //  What do we need?
  var doc = context.document;
  var pages = [doc pages];

  // Loop through the pages to create the initial structure
  for (var i=0; i<[pages count]; i++) {
    var page = pages[i];
    var artboards = [page artboards];
    log(page.name());
    log([artboards count]);
  }
  
};
